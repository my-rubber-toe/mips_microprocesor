#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x564e9e043280 .scope module, "main" "main" 2 10;
 .timescale 0 0;
P_0x564e9e043400 .param/l "sim_time" 0 2 21, +C4<00000000000000000000011101101100>;
v0x564e9e063850_0 .var "ins", 31 0;
v0x564e9e063940_0 .net "result", 31 0, v0x564e9e063710_0;  1 drivers
S_0x564e9e0434a0 .scope module, "shift2" "shiftLeft2" 2 24, 2 1 0, S_0x564e9e043280;
 .timescale 0 0;
    .port_info 0 /OUTPUT 32 "result"
    .port_info 1 /INPUT 32 "in"
v0x564e9e043670_0 .net "in", 31 0, v0x564e9e063850_0;  1 drivers
v0x564e9e063710_0 .var "result", 31 0;
E_0x564e9e03e280 .event edge, v0x564e9e043670_0;
    .scope S_0x564e9e0434a0;
T_0 ;
    %wait E_0x564e9e03e280;
    %load/vec4 v0x564e9e043670_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x564e9e063710_0, 0, 32;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x564e9e043280;
T_1 ;
    %delay 1900, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x564e9e043280;
T_2 ;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x564e9e063850_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 34 "$display", "\000" {0 0 0};
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x564e9e063850_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 37 "$display", "\000" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x564e9e043280;
T_3 ;
    %vpi_call 2 45 "$display", "time        ins          result" {0 0 0};
    %vpi_call 2 46 "$monitor", "%4d        %b           %b  ", $time, v0x564e9e063850_0, v0x564e9e063940_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "shiftLeft2.v";
